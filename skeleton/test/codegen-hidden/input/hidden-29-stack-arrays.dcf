class Program {
int A[10];

void test(int d)
{
  int i;

  if (d == 4) { return ; }
  for i = 0, 10 
  {
    A[i] = i % d;  
  }

  test(d + 1);
  callout("printf", "result should be: ");
  for i = 0, 10 {
    callout("printf", "%d ", A[i] );
  }
  callout("printf", "\n");
}

void main () {
  test(1);
}

}
