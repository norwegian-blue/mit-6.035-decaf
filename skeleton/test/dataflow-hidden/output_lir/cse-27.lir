    NODE ENTRY
    NODE STMT	_tmp2 ASSIGN get_int(2)
    NODE STMT	_foo3_c ASSIGN 0
    NODE STMT	_foo7_i ASSIGN 1
		GOTO   --> foo2

BLOCK foo2    
    NODE CMP	 ((_foo7_i LT 10))
		COND T --> foo3
		COND F --> foo4

BLOCK foo3    
    NODE STMT	_tmp3 ASSIGN (_foo7_i TIMES _foo7_i)
    NODE STMT	_tmp4 ASSIGN (3 PLUS _tmp2)
    NODE STMT	_tmp5 ASSIGN (_tmp3 TIMES _tmp4)
    NODE STMT	_foo3_c ASSIGN (_foo3_c PLUS _tmp5)
    NODE STMT	_foo7_i ASSIGN (_foo7_i PLUS 1)
		GOTO   --> foo2

BLOCK foo4    
    NODE EXIT	return _foo3_c


