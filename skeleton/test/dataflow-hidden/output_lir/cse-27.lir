################# Variable renaming (no duplicate names) #################
CLASS Program
fields:
methods:
METHOD (INT) get_int((INT) _get_int1_x)
  RETURN _get_int1_x

METHOD (BOOL) get_bool((BOOL) _get_bool1_x)
  RETURN _get_bool1_x

METHOD (INT) foo()
  (INT) _foo1_a
  (INT) _foo2_b
  (INT) _foo3_c
  (INT) _foo4_d
  (INT) _foo5_i
  (BOOL) _foo6_x
  _foo1_a ASSIGN get_int(7)
  _foo2_b ASSIGN get_int(2)
  _foo6_x ASSIGN get_bool(true)
  _foo3_c ASSIGN 0
  _foo4_d ASSIGN (_foo1_a PLUS _foo2_b)
  FOR (_foo7_i = 1 to 10) {  
    _foo1_a ASSIGN 3
    _foo3_c ASSIGN (_foo3_c PLUS ((_foo7_i TIMES _foo7_i) TIMES (_foo1_a PLUS _foo2_b)))
  }
  RETURN _foo3_c

METHOD (VOID) main()
  (INT) _main1_c
  _main1_c ASSIGN foo()
  Callout("printf", "%%d\n", _main1_c)

############ Blockified Control Flow Graph ############
foo:
BLOCK foo1    
    NODE ENTRY
    NODE STMT	_tmp6 ASSIGN get_int(7)
    NODE STMT	_foo1_a ASSIGN _tmp6
    NODE STMT	_tmp2 ASSIGN get_int(2)
    NODE STMT	_foo2_b ASSIGN _tmp2
    NODE STMT	_tmp1 ASSIGN get_bool(true)
    NODE STMT	_foo6_x ASSIGN _tmp1
    NODE STMT	_foo3_c ASSIGN 0
    NODE STMT	_foo4_d ASSIGN (_foo1_a PLUS _foo2_b)
    NODE STMT	_foo7_i ASSIGN 1
		GOTO   --> foo2

BLOCK foo2    
    NODE CMP	 ((_foo7_i LT 10))
		COND T --> foo3
		COND F --> foo4

BLOCK foo3    
    NODE STMT	_foo1_a ASSIGN 3
    NODE STMT	_tmp3 ASSIGN (_foo7_i TIMES _foo7_i)
    NODE STMT	_tmp4 ASSIGN (_foo1_a PLUS _foo2_b)
    NODE STMT	_tmp5 ASSIGN (_tmp3 TIMES _tmp4)
    NODE STMT	_foo3_c ASSIGN (_foo3_c PLUS _tmp5)
    NODE STMT	_foo7_i ASSIGN (_foo7_i PLUS 1)
		GOTO   --> foo2

BLOCK foo4    
    NODE EXIT	return _foo3_c


get_bool:
BLOCK get_bool1    
    NODE ENTRY
    NODE EXIT	return _get_bool1_x


main:
BLOCK main1    
    NODE ENTRY
    NODE STMT	_tmp1 ASSIGN foo()
    NODE STMT	_main1_c ASSIGN _tmp1
    NODE STMT	Callout("printf", "%%d\n", _main1_c)
    NODE EXIT


get_int:
BLOCK get_int1    
    NODE ENTRY
    NODE EXIT	return _get_int1_x



############ Optimized Control Flow Graph ############
