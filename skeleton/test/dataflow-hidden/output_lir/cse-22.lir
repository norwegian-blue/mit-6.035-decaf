    NODE STMT	_glb1_a[_tmp5] ASSIGN 7
    NODE STMT	Callout("printf", "%%d\n", _tmp5)
    NODE STMT	_tmp1 ASSIGN _glb1_a[5]
    NODE STMT	Callout("printf", "%%d\n", _tmp1)
    NODE EXIT


get_int:
BLOCK get_int1    
    NODE ENTRY
    NODE EXIT	return _get_int1_x



