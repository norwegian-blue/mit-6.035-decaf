################# Variable renaming (no duplicate names) #################
CLASS Program
fields:
methods:
METHOD (INT) get_int((INT) _get_int1_x)
  RETURN _get_int1_x

METHOD (BOOL) get_bool((BOOL) _get_bool1_x)
  RETURN _get_bool1_x

METHOD (INT) foo()
  (INT) _foo1_a
  (INT) _foo2_b
  (INT) _foo3_c
  (BOOL) _foo4_x
  _foo1_a ASSIGN get_int(7)
  _foo2_b ASSIGN get_int(2)
  _foo4_x ASSIGN get_bool(true)
  _foo3_c ASSIGN 0
  _foo3_c ASSIGN (_foo1_a PLUS _foo2_b)
  IF (_foo4_x) {  
    RETURN (_foo1_a PLUS _foo2_b)
  }
  RETURN (_foo3_c MINUS 3)

METHOD (VOID) main()
  (INT) _main1_c
  _main1_c ASSIGN foo()
  Callout("printf", "%%d\n", _main1_c)

############ Blockified Control Flow Graph ############
foo:
BLOCK foo1    
    NODE ENTRY
    NODE STMT	_tmp2 ASSIGN get_int(7)
    NODE STMT	_foo1_a ASSIGN _tmp2
    NODE STMT	_tmp1 ASSIGN get_int(2)
    NODE STMT	_foo2_b ASSIGN _tmp1
    NODE STMT	_tmp4 ASSIGN get_bool(true)
    NODE STMT	_foo4_x ASSIGN _tmp4
    NODE STMT	_foo3_c ASSIGN 0
    NODE STMT	_foo3_c ASSIGN (_foo1_a PLUS _foo2_b)
    NODE CMP	 (_foo4_x)
		COND T --> foo2
		COND F --> foo3

BLOCK foo2    
    NODE STMT	_tmp5 ASSIGN (_foo1_a PLUS _foo2_b)
    NODE EXIT	return _tmp5

BLOCK foo3    
    NODE STMT	_tmp3 ASSIGN (_foo3_c MINUS 3)
    NODE EXIT	return _tmp3


get_bool:
BLOCK get_bool1    
    NODE ENTRY
    NODE EXIT	return _get_bool1_x


main:
BLOCK main1    
    NODE ENTRY
    NODE STMT	_tmp1 ASSIGN foo()
    NODE STMT	_main1_c ASSIGN _tmp1
    NODE STMT	Callout("printf", "%%d\n", _main1_c)
    NODE EXIT


get_int:
BLOCK get_int1    
    NODE ENTRY
    NODE EXIT	return _get_int1_x



############ Optimized Control Flow Graph ############
foo:
BLOCK foo1    
    NODE ENTRY
    NODE STMT	_tmp2 ASSIGN get_int(7)
    NODE STMT	_tmp1 ASSIGN get_int(2)
    NODE STMT	_tmp4 ASSIGN get_bool(true)
    NODE STMT	_tmp6 ASSIGN (_tmp2 PLUS _tmp1)
    NODE CMP	 (_tmp4)
		COND T --> foo2
		COND F --> foo3

BLOCK foo2    
    NODE STMT	_tmp5 ASSIGN _tmp6
    NODE EXIT	return _tmp5

BLOCK foo3    
    NODE STMT	_tmp3 ASSIGN (_tmp6 MINUS 3)
    NODE EXIT	return _tmp3


get_bool:
BLOCK get_bool1    
    NODE ENTRY
    NODE EXIT	return _get_bool1_x


main:
BLOCK main1    
    NODE ENTRY
    NODE STMT	_tmp1 ASSIGN foo()
    NODE STMT	Callout("printf", "%%d\n", _tmp1)
    NODE EXIT


