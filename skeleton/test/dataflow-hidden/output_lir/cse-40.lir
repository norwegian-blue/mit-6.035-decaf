################# Variable renaming (no duplicate names) #################
CLASS Program
fields:
methods:
METHOD (INT) get_int((INT) _get_int1_x)
  RETURN _get_int1_x

METHOD (BOOL) get_bool((BOOL) _get_bool1_x)
  RETURN _get_bool1_x

METHOD (INT) foo()
  (INT) _foo1_a
  (INT) _foo2_b
  (INT) _foo3_c
  (INT) _foo4_i
  (INT) _foo5_d
  (BOOL) _foo6_x
  _foo6_x ASSIGN get_bool(true)
  _foo4_i ASSIGN get_int(0)
  _foo5_d ASSIGN (_foo1_a PLUS _foo2_b)
  _foo3_c ASSIGN (_foo1_a PLUS _foo2_b)
  IF (((_foo3_c MINUS _foo5_d) EQ 0)) {  
    _foo4_i ASSIGN 1
  }
  RETURN _foo4_i

METHOD (VOID) main()
  (INT) _main1_c
  _main1_c ASSIGN foo()
  Callout("printf", "%%d\n", _main1_c)

############ Blockified Control Flow Graph ############
foo:
BLOCK foo1    
    NODE ENTRY
    NODE STMT	_tmp3 ASSIGN get_bool(true)
    NODE STMT	_foo6_x ASSIGN _tmp3
    NODE STMT	_tmp1 ASSIGN get_int(0)
    NODE STMT	_foo4_i ASSIGN _tmp1
    NODE STMT	_foo5_d ASSIGN (_foo1_a PLUS _foo2_b)
    NODE STMT	_foo3_c ASSIGN (_foo1_a PLUS _foo2_b)
    NODE STMT	_tmp2 ASSIGN (_foo3_c MINUS _foo5_d)
    NODE CMP	 ((_tmp2 EQ 0))
		COND T --> foo2
		COND F --> foo3

BLOCK foo2    
    NODE STMT	_foo4_i ASSIGN 1
		GOTO   --> foo3

BLOCK foo3    
    NODE EXIT	return _foo4_i


get_bool:
BLOCK get_bool1    
    NODE ENTRY
    NODE EXIT	return _get_bool1_x


main:
BLOCK main1    
    NODE ENTRY
    NODE STMT	_tmp1 ASSIGN foo()
    NODE STMT	_main1_c ASSIGN _tmp1
    NODE STMT	Callout("printf", "%%d\n", _main1_c)
    NODE EXIT


get_int:
BLOCK get_int1    
    NODE ENTRY
    NODE EXIT	return _get_int1_x



############ Optimized Control Flow Graph ############
foo:
BLOCK foo1    
    NODE ENTRY
    NODE STMT	_tmp1 ASSIGN get_int(0)
    NODE STMT	_foo4_i ASSIGN _tmp1
    NODE CMP	 (true)
		COND T --> foo2
		COND F --> foo3

BLOCK foo2    
    NODE STMT	_foo4_i ASSIGN 1
		GOTO   --> foo3

BLOCK foo3    
    NODE EXIT	return _foo4_i


get_bool:
BLOCK get_bool1    
    NODE ENTRY
    NODE EXIT	return _get_bool1_x


main:
BLOCK main1    
    NODE ENTRY
    NODE STMT	_tmp1 ASSIGN foo()
    NODE STMT	Callout("printf", "%%d\n", _tmp1)
    NODE EXIT


get_int:
BLOCK get_int1    
    NODE ENTRY
    NODE EXIT	return _get_int1_x



