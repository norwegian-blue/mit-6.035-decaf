CLASS Program
fields:
methods:
METHOD (INT) get_int((INT) _get_int1_x)
  RETURN _get_int1_x

METHOD (VOID) main()
  (INT) _main1_a
  (INT) _main2_b
  (INT) _main3_c
  (INT) _main4_d
  _main1_a ASSIGN get_int(2)
  _main2_b ASSIGN get_int(3)
  _main3_c ASSIGN 0
  _main4_d ASSIGN 0
  _main3_c ASSIGN ((_main1_a PLUS _main2_b) TIMES (_main1_a PLUS _main2_b))
  _main4_d ASSIGN ((_main1_a PLUS _main2_b) DIVIDE (_main1_a PLUS _main2_b))
  Callout("printf", "%%d\n", _main3_c)
  Callout("printf", "%%d\n", _main4_d)

############ Blockified Control Flow Graph ############
