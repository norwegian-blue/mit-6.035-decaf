    NODE STMT	_main2_b ASSIGN _tmp1
    NODE STMT	_main3_c ASSIGN 0
    NODE STMT	_main4_d ASSIGN 0
    NODE STMT	_tmp2 ASSIGN (_main1_a PLUS _main2_b)
    NODE STMT	_tmp3 ASSIGN (_main1_a PLUS _main2_b)
    NODE STMT	_main3_c ASSIGN (_tmp2 TIMES _tmp3)
    NODE STMT	_tmp4 ASSIGN (_main1_a PLUS _main2_b)
    NODE STMT	_tmp5 ASSIGN (_main1_a PLUS _main2_b)
    NODE STMT	_main4_d ASSIGN (_tmp4 DIVIDE _tmp5)
    NODE STMT	Callout("printf", "%%d\n", _main3_c)
    NODE STMT	Callout("printf", "%%d\n", _main4_d)
    NODE EXIT


