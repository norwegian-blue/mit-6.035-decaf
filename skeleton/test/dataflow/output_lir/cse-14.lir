################# Variable renaming (no duplicate names) #################
CLASS Program
fields:
methods:
METHOD (INT) get_int((INT) _get_int1_x)
  RETURN _get_int1_x

METHOD (VOID) main()
  (INT) _main1_a
  (INT) _main2_b
  (INT) _main3_c
  (INT) _main4_d
  (INT) _main5_x
  (INT) _main6_i
  _main1_a ASSIGN get_int(2)
  _main2_b ASSIGN get_int(3)
  _main5_x ASSIGN get_int(4)
  _main3_c ASSIGN 0
  _main4_d ASSIGN 0
  _main3_c ASSIGN (_main1_a PLUS _main2_b)
  FOR (_main7_i = 5 to _main5_x) {  
    _main4_d ASSIGN (_main1_a PLUS _main2_b)
  }
  Callout("printf", "%%d\n", _main3_c)
  Callout("printf", "%%d\n", _main4_d)

############ Blockified Control Flow Graph ############
main:
BLOCK main1    
    NODE ENTRY
    NODE STMT	_tmp1 ASSIGN get_int(2)
    NODE STMT	_main1_a ASSIGN _tmp1
    NODE STMT	_tmp3 ASSIGN get_int(3)
    NODE STMT	_main2_b ASSIGN _tmp3
    NODE STMT	_tmp2 ASSIGN get_int(4)
    NODE STMT	_main5_x ASSIGN _tmp2
    NODE STMT	_main3_c ASSIGN 0
    NODE STMT	_main4_d ASSIGN 0
    NODE STMT	_main3_c ASSIGN (_main1_a PLUS _main2_b)
    NODE STMT	_main7_i ASSIGN 5
		GOTO   --> main2

BLOCK main2    
    NODE CMP	 ((_main7_i LT _main5_x))
		COND T --> main3
		COND F --> main4

BLOCK main3    
    NODE STMT	_main4_d ASSIGN (_main1_a PLUS _main2_b)
    NODE STMT	_main7_i ASSIGN (_main7_i PLUS 1)
		GOTO   --> main2

BLOCK main4    
    NODE STMT	Callout("printf", "%%d\n", _main3_c)
    NODE STMT	Callout("printf", "%%d\n", _main4_d)
    NODE EXIT


get_int:
BLOCK get_int1    
    NODE ENTRY
    NODE EXIT	return _get_int1_x



